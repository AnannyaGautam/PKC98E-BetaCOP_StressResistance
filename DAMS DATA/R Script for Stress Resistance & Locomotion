# We also did Geno-Diet specific Locomotor Activity Data Analysis. 
# For starvation we choose LANDMARK_H <- 48 (as this has 2 x 12:12hours Light and Dark Cycle) 
# For Paraquat we choose to do a Landmark Analysis, we decided on time when >95% of the Individuals were alive 

# To select a Landmark Time for PQ 
 km_all  <- survfit(Surv(time_hr, event) ~ 1, data = per_fly)
 S_all   <- data.frame(time = km_all$time, surv = km_all$surv)
 t_95_all <- suppressWarnings(min(S_all$time[S_all$surv < 0.95], na.rm = TRUE))
 
 LANDMARK_H <- floor(t_95_all)
act_src <- activity_long %>%
  # time per bin is already "time_hr"
  rename(time_bin_hr = time_hr) %>%
  # parse "G####_D##_V#" into columns
  extract(condition, into = c("Genotype","Diet","Vial"),
          regex = "G(\\d+)_D(\\d+)_V(\\d+)", remove = FALSE, convert = TRUE) %>%
  mutate(
    Monitor  = as.integer(Monitor),
    Channel  = as.integer(Channel),
    Genotype = as.character(Genotype),
    Diet     = as.integer(Diet),
    Vial     = as.integer(Vial)
  ) %>%
  arrange(Monitor, Channel, time_bin_hr) %>%
  group_by(Monitor, Channel) %>%
  # robust dt: use forward difference in time; fallback to global median step if needed
  mutate(dt_hr = lead(time_bin_hr) - time_bin_hr) %>%
  ungroup()


early <- act_src %>%
  dplyr::filter(time_bin_hr <= LANDMARK_H) %>%
  dplyr::mutate(dt_hr = bin_minutes/60) %>%
  dplyr::group_by(Monitor, Channel, Genotype, Diet) %>%
  dplyr::summarise(
    AUC_early  = sum(activity * dt_hr, na.rm = TRUE),
    rate_early = AUC_early / LANDMARK_H,
    .groups = "drop"
  ) %>%
  dplyr::mutate(Diet_f = factor(Diet, levels = c(5,20), labels = c("5%","20%")))

# Now the early, contains the acitivity of the specific LANDMARK_H defined. 
#Now you can subset the data for your own specifc genos and analysis is done by lmer and estimates are calculated with emmeans 
