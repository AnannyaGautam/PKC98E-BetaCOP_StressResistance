This folder contains processed datasets from the Drosophila Activity Monitoring System (DAMS) used to assess stress resistance under starvation and paraquat-induced oxidative stress. Each file represents individual fly-level survival data recorded as time-to-death (hours) and corresponding metadata (genotype, diet, monitor ID, and replicate).

#We first created a script to create a list for Randomisation in DAMS for each monitor, to exclude any row or column effects per monitor caused. 

library(tidyverse)      
set.seed(46)             # reproducible 

# Layout constants
n_monitors            <- 15
rows_per_monitor      <- 4
cols_per_row          <- 8
rows_lab              <- LETTERS[1:rows_per_monitor]            # A, B, C, D
cols_lab              <- paste0("Col", 8:1)                     # flipped: Col8 … Col1
wells_per_monitor     <- rows_per_monitor * cols_per_row        # 32

reps_per_cond_per_mon <- 3
n_blanks_per_monitor  <- wells_per_monitor - (10 * reps_per_cond_per_mon)  # 2
stopifnot(n_blanks_per_monitor >= 0)

# 10 conditions = 5 genotypes x 2 diets
conditions <- tribble(
  ~ConditionID, ~Genotype, ~Diet,
  1, "1008", "5",
  2, "1008", "20",
  3, "1007", "5",
  4, "1007", "20",
  5, "1477", "5",
  6, "1477", "20",
  7, "1491", "5",
  8, "1491", "20",
  9, "7791", "5",
 10, "7791", "20"
) %>%
  mutate(Condition = str_glue("G{Genotype}_D{Diet}"))  # compact human label

# For each condition, assign exactly one vial (1..5) to each monitor, with balance:
# Each vial is used on exactly 3 monitors (15 monitors / 5 vials = 3 each).
vial_assignments <- map_dfr(conditions$ConditionID, function(cid) {
  tibble(
    ConditionID = cid,
    Monitor     = 1:n_monitors,
    VialID      = sample(rep(6:10, each = n_monitors / 5))  # 1..5 each exactly 3 times, shuffled
  )
})

make_monitor_wells <- function(m_idx) {
  # For this monitor: get vial choice for each condition
  va_m <- vial_assignments %>% filter(Monitor == m_idx)
samples <- conditions %>%
  left_join(va_m, by = "ConditionID") %>%
  arrange(ConditionID) %>%
  tidyr::uncount(weights = reps_per_cond_per_mon, .remove = FALSE) %>%
  group_by(ConditionID, Genotype, Diet, VialID) %>%
  mutate(Rep = row_number()) %>%
  ungroup() %>%
  mutate(
    SampleLabel = paste0("G", Genotype, "_D", Diet, "_V", VialID)
  ) %>%
  select(ConditionID, Genotype, Diet, VialID, Rep, SampleLabel, Condition)

blanks <- tibble(
  ConditionID = rep(NA_integer_, n_blanks_per_monitor),
  Genotype    = rep(NA_character_, n_blanks_per_monitor),
  Diet        = rep(NA_character_, n_blanks_per_monitor),
  VialID      = rep(NA_integer_, n_blanks_per_monitor),
  Rep         = rep(NA_integer_, n_blanks_per_monitor),
  SampleLabel = paste0("Blank", seq_len(n_blanks_per_monitor)),
  Condition   = rep("Blank", n_blanks_per_monitor)
)
  entries <- bind_rows(samples, blanks) %>% slice_sample(prop = 1)
  wells <- expand_grid(Row = rows_lab, Column = cols_lab) %>%
    mutate(Monitor = m_idx) %>%
    bind_cols(entries) %>%
    relocate(Monitor, Row, Column)

  wells
}

wells_all <- map_dfr(1:n_monitors, make_monitor_wells)

# Wide grid with one row per (Monitor, Row A–D), cols Col8..Col1 filled with SampleLabel
grid_view <- wells_all %>%
  select(Monitor, Row, Column, SampleLabel) %>%
  pivot_wider(names_from = Column, values_from = SampleLabel) %>%
  arrange(Monitor, Row)

write_csv(grid_view, "DAM_grid_by_monitor.csv")


#master script to read the files and parse the data
```{r}

rm(list = ls())

DATA_DIR <- "D:/DAMS/Paraquat Assay 21082025/Analysis"
OUTPUT_DIR <- file.path(DATA_DIR, "Output")
SAVE_DIR <- file.path(DATA_DIR, "Paraquat")


if (!dir.exists(SAVE_DIR)) {
  dir.create(SAVE_DIR)
}

#List monitor files 
fn.vec <- list.files(OUTPUT_DIR, pattern = "MyRun.*CtM.*\\.txt$", full.names = FALSE)
if (length(fn.vec) == 0) {
  stop("No monitor output files found in the Output folder. Check the path and file names.")
}

#Full file paths to monitor files
fp.vec <- file.path(OUTPUT_DIR, fn.vec)

file.dat <- lapply(fp.vec, read.table, sep = "\t", header = FALSE)
names(file.dat) <- fn.vec
cat("Files found:\n")
print(fn.vec)
cat("Number of files read:", length(file.dat), "\n")

file.dat = lapply(fp.vec, read.table, sep = "\t", header = FALSE)
names(file.dat) = fn.vec
mon.dat = lapply(file.dat, function(tab) tab[,-(1:10)])
active.rows = lapply(mon.dat, function(tab) which(apply(tab, 1, function(rw) !all(rw == 0)))) 
all.first.rows = sapply(active.rows, function(x) x[1])
if (length(unique(all.first.rows)) > 1) print("WARNING! Montior reads don't start at the same time. Check files.")

# Find per-monitor first and last active rows
first_i <- vapply(active.rows, function(x) x[1], integer(1))
last_i  <- vapply(active.rows, function(x) max(x) + 1L, integer(1))  # include a trailing bin

# Trim each monitor to its own active window
trunc.dat <- lapply(seq_along(mon.dat), function(i) {
  mon.dat[[i]][ first_i[i] : last_i[i], , drop = FALSE ]
})
trunc.dm <- lapply(trunc.dat, data.matrix)

# Per-monitor "last alive" index (relative to each monitor's own start)
threshold <- 2
y <- lapply(trunc.dm, function(x) {
  apply(x, 2, function(w) {
    idx <- tail(which(w > threshold), 1)
    if (length(idx) == 0) NA_integer_ else idx
  })
})
setwd(DATA_DIR)
Monitorinfo <- read.csv("Monitor.info.csv")
monitor.list = lapply(seq_along(y), function(i) data.frame(lastalive=y[[i]], condition=Monitorinfo[1:ncol(trunc.dm[[i]]), i],Monitor=i)) 

library(tidyverse)

bin_minutes <- 5
rows_per_day <- 24 * 60 / bin_minutes   # 288 when bin = 5 min

channels_per_monitor <- ncol(trunc.dm[[1]])  # should be 32

per_fly <- purrr::map_dfr(seq_along(trunc.dm), function(i) {
  labels_i <- Monitorinfo[seq_len(channels_per_monitor), i]
  tibble(
    Monitor   = i,
    Channel   = seq_len(channels_per_monitor),
    condition = labels_i,
    last_idx  = y[[i]],                       # last-active index in this monitor's window
    n_bins    = nrow(trunc.dm[[i]]),
    time_hr   = last_idx * bin_minutes / 60,
    event     = as.integer(last_idx < n_bins), # 1=dead, 0=censored
  )
})

# Parse G####_D##_V# 
per_fly <- per_fly %>%
  tidyr::extract(condition, c("Genotype","Diet","Vial"),
                 regex = "G(\\d+)_D(\\d+)_V(\\d+)", remove = FALSE, convert = TRUE) %>%
  mutate(
    Diet_lab = factor(ifelse(is.na(Diet), NA, paste0(Diet, "%"))),
    Genotype = factor(Genotype),
    Vial     = as.integer(Vial)
  )

library(dplyr)
library(tidyr)
library(ggplot2)
library(survival)
library(survminer)
library(coxme)
library(patchwork)
library(broom)
library(stringr)

