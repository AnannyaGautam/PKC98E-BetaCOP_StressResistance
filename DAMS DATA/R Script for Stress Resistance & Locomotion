                   data = df_pair_dKD) 

coxme_fit_dKD_5 <- coxme(Surv(time_hr, event) ~ Genotype  + (1|Monitor),
                   data = df_pair_dKD_5)

coxme_fit_dKD_20 <- coxme(Surv(time_hr, event) ~ Genotype  + (1|Monitor),
                   data = df_pair_dKD_20)

